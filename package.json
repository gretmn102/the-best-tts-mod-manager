{
  "name": "the-best-tts-mod-manager-ever",
  "version": "0.0.1",
  "description": "Full featured solution with GUI to backup and install modules of Tabletop Simulator",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --color",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --color",
    "test": "jest",
    "build:win": "electron-builder build --win --x64",
    "build:mac": "electron-builder build --mac --x64",
    "lint": "eslint . --ext .ts"
  },
  "build": {
    "appId": "com.fering.the-best-tts-mod-manager-ever",
    "productName": "Tabletop Simulation Mod Manager",
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis",
      "asar": true
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "buildVersion": "1"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.1",
    "fp-ts": "^2.11.1",
    "fs-extra": "^10.0.0",
    "got": "^11.8.2",
    "platform-folders": "^0.5.4",
    "rage-edit": "^1.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.1.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "ts-custom-error": "^3.2.0"
  },
  "devDependencies": {
    "@node-steam/vdf": "^2.2.0",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.15",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.7",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "electron-reloader": "^1.2.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^27.1.0",
    "lodash": "^4.17.20",
    "rimraf": "^3.0.2",
    "source-map-loader": "^2.0.0",
    "spectron": "^13.0.0",
    "style-loader": "^2.0.0",
    "ts-jest": "27.0.5",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gretmn102/the-best-tts-mod-manager-ever"
  },
  "keywords": [
    "TTS",
    "Tabletop Simulator",
    "Mod Manager",
    "Electron",
    "React",
    "Redux",
    "Typescript",
    "Webpack"
  ],
  "bugs": {
    "url": "https://github.com/gretmn102/the-best-tts-mod-manager-ever/issues"
  },
  "homepage": "https://github.com/gretmn102/the-best-tts-mod-manager-ever#readme"
}
